    "author": "Martin Guillon<martin.guillon@akylas.fr>",
    "scripts": {
        "pandroid": "cross-var tns build android --bundle --release --keyStorePath $KEYSTORE_PATH --keyStorePassword $KEYSTORE_PASS --keyStoreAlias $KEYSTORE_ALIAS --keyStoreAliasPassword $KEYSTORE_PASS --env.uglify --env.production --env.snapshot",
        "pios": "tns build ios --bundle --release --env.uglify --for-device --provision 'Cairn Mobile'",
        "buildtest": "npm run pandroid -- --aab --env.adhoc; npm run pios -- --env.adhoc",
        "webpack": "webpack --env.ios",
        "run.android.prod": "tns run android --bundle --release --keyStorePath $KEYSTORE_PATH --keyStorePassword $KEYSTORE_PASS --keyStoreAlias $KEYSTORE_ALIAS --keyStoreAliasPassword $KEYSTORE_PASS  --env.production --env.uglify --env.snapshot",
        "run.ios.prod": "tns run ios --bundle --release --env.uglify  --env.production --for-device --provision 'Cairn Mobile'"
    },
