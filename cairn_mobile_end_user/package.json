{
    "description": "eCairn",
    "readme": "NativeScript Application",
    "repository": "https://github.com/cairn-monnaie/CairnMobile",
    "bugs": {
        "url": "https://github.com/cairn-monnaie/CairnMobile/issues"
    },
    "license": "Apache-2.0",
    "nativescript": {
        "id": "com.akylas.cairnmobile",
        "tns-ios": {
            "version": "6.4.2"
        },
        "tns-android": {
            "version": "6.4.1"
        }
    },
    "author": "Martin Guillon<martin.guillon@akylas.fr>",
    "scripts": {
        "run.android.production": "cross-var tns run android --release --keyStorePath $KEYSTORE_PATH --keyStorePassword $KEYSTORE_PASSWORD --keyStoreAlias $KEYSTORE_ALIAS --keyStoreAliasPassword $KEYSTORE_ALIAS_PASSWORD --env.adhoc  --env.snapshot --env.compileSnapshot",
        "pandroid": "cross-var tns build android --release --keyStorePath $KEYSTORE_PATH --keyStorePassword $KEYSTORE_PASSWORD --keyStoreAlias $KEYSTORE_ALIAS --keyStoreAliasPassword $KEYSTORE_ALIAS_PASSWORD --env.adhoc --env.snapshot --env.compileSnapshot",
        "pios": "tns build ios --release --env.adhoc --env.snapshot --env.compileSnapshot --for-device --provision 'match AppStore com.akylas.cairnmobile 1590395309'",
        "run.ios.prod": "tns run ios --env.adhoc --release --for-device --provision 'match AdHoc com.akylas.cairnmobile'",
        "ios.generate_licences": "node generate_ios_licences.js",
        "android.generate_licences": "platforms/android/gradlew  generateLicenseReport "
    },
